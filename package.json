{
  "name": "learn-itermit",
  "version": "0.0.0",
  "engines": {
    "node": ">=16.0.0 <20.11.0",
    "yarn": ">=1.22.0 <2",
    "npm": "Please use Yarn instead of NPM to install dependencies"
  },
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "start:prod": "ng serve --configuration=production",
    "start:dev": "ng serve --configuration=development",
    "start:test": "ng serve --configuration=testing",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "build:prod": "yarn build --configuration=production",
    "build:dev": "yarn build --configuration=development",
    "build:test": "yarn build --configuration=testing",
    "lint": "npm run lint:ts && npm run lint:scss",
    "lint:ts": "eslint \"src/**/*.ts\" --fix",
    "lint:scss": "stylelint \"src/**/*.scss\" --fix",
    "hmr": "ng serve --hmr --disable-host-check",
    "predeploy": "npm run build:prod",
    "deploy": "ng deploy"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^17.3.1",
    "@angular/cdk": "^17.3.1",
    "@angular/common": "^17.3.1",
    "@angular/compiler": "^17.3.1",
    "@angular/core": "^17.3.1",
    "@angular/forms": "^17.3.1",
    "@angular/material": "^17.3.1",
    "@angular/material-experimental": "^17.3.1",
    "@angular/material-moment-adapter": "^17.3.1",
    "@angular/platform-browser": "^17.3.1",
    "@angular/platform-browser-dynamic": "^17.3.1",
    "@angular/router": "^17.3.1",
    "@ng-matero/extensions": "^17.2.0",
    "@ng-matero/extensions-moment-adapter": "^17.1.2",
    "@ngx-translate/core": "^15.0.0",
    "@ngx-translate/http-loader": "^8.0.0",
    "@tinymce/tinymce-angular": "^7.0.0",
    "moment": "^2.30.1",
    "ng-lazyload-image": "^9.1.3",
    "ngx-mask": "^17.0.4",
    "ngx-permissions": "^17.1.0",
    "ngx-progressbar": "^11.1.0",
    "ngx-toastr": "^18.0.0",
    "rxjs": "^7.8.1",
    "screenfull": "^6.0.2",
    "snack-helper": "^2.2.0",
    "tinymce": "^7.0.0",
    "tslib": "^2.6.2",
    "zone.js": "^0.14.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^17.3.1",
    "@angular-eslint/builder": "^17.3.0",
    "@angular-eslint/eslint-plugin": "^17.3.0",
    "@angular-eslint/eslint-plugin-template": "^17.3.0",
    "@angular-eslint/schematics": "^17.3.0",
    "@angular-eslint/template-parser": "^17.3.0",
    "@angular/cli": "^17.3.1",
    "@angular/compiler-cli": "^17.3.1",
    "@types/jasmine": "^5.1.4",
    "@typescript-eslint/eslint-plugin": "^7.3.1",
    "@typescript-eslint/parser": "^7.3.1",
    "eslint": "^8.57.0",
    "jasmine-core": "^5.1.2",
    "jasmine-spec-reporter": "^7.0.0",
    "karma": "^6.4.3",
    "karma-chrome-launcher": "^3.2.0",
    "karma-coverage": "^2.2.1",
    "karma-jasmine": "^5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "karma-junit-reporter": "^2.0.1",
    "ngx-spec": "^2.1.6",
    "prettier": "^3.2.5",
    "stylelint": "^16.2.1",
    "stylelint-config-recess-order": "^5.0.0",
